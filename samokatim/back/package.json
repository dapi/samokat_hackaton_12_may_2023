{
  "name": "company",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug 9230 --watch",
    "start:prod": "node dist/main",
    "lint": "tslint --fix --project .",
    "test": "jest --config ./test/jest-e2e.json",
    "docker": "docker-compose up",
    "typeorm": "npm run build && npx typeorm -d dist/data-source.js",
    "migration:create": "npx typeorm migration:create",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "docker:build": "docker build . -t nexus.caaat.pro/repository/docker-projects/company-api:latest",
    "docker:push": " docker push nexus.caaat.pro/repository/docker-projects/company-api:latest"
  },
  "dependencies": {
    "@types/multer": "^1.4.7",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/passport-jwt": "^3.0.7",
    "bcrypt": "^5.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "passport-jwt": "^4.0.0",
    "rimraf": "^3.0.2",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/jest": "29.1.2",
    "@types/node": "^18.8.2",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "base64-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "puppeteer": "^18.2.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
